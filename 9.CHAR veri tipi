public class charr {

	public static void main(String[] args) {
		
		// karakter tanımlamak için kullanılır 2 byte 
		
		char karakter = '-';
		
		// char veri tipi ile tanımlanan değerler tek tırnak içinde olmalıdır 
    
		// char veri tipi tek bir karakteri tutabilir 
		
		System.out.println(karakter);
	}

}
