public class DOU {
	
	public static void main(String[] args) {
		
     // ondalıklı sayı tanımlamak için kullanılır
		
		// noktadan sonra 17 basamak çıkılabilir 8 byte
		
		float sayi1 =9.123456789123456789f;
				
		double sayi2 = 9.12345678912345;
		System.out.println(sayi1);
		System.out.println(sayi2);

	}
}
